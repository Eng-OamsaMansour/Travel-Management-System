    private void loginActionPerformed(java.awt.event.ActionEvent evt) {                                      
//login function call 
String n=new String(name.getText());
if(n.equals("M"))
   new manager().setVisible(true); 
else if(n.equals("R"))
new reception().setVisible(true);
else 
new Firstpage().setVisible(true);
    }                                     

